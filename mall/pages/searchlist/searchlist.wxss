/* pages/searchlist/searchlist.wxss */
.goodcontainer {
  display: flex;
  flex-wrap: wrap;
}

.good {
  width: 50%;
  padding: 20rpx;
  box-sizing: border-box;
  text-align: center;
}

.good image {
  width: 100rpx;
}

.sortlist {
  display: flex;
  height:100rpx;
  line-height: 100rpx;
  background-color: rgb(240, 240, 240);
}

.sortlist view {
  flex: 1;
  text-align: center;
}

.active {
  color:red;
}